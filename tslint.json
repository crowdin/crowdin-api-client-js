{
    "rules": {
        "no-string-throw": true,
        "no-unused-expression": true,
        "no-duplicate-variable": true,
        "curly": true,
        "class-name": true,
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": true,
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "indent": [
            true,
            "spaces",
            4
        ]
    },
    "defaultSeverity": "warning"
}